<%- include ../partials\header.ejs %>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="newItem">Title</label>
    <input type="text" name="newItem" class="form-control" />
    <label for="newContent"></label>
    <textarea
      name="newContent"
      rows="5"
      cols="80"
      class="form-control"
    ></textarea>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </div>
</form>
